<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- classPathEntry location="${mybatis.jdbc.classpath}" / -->

	<context id="demo1" targetRuntime="MyBatis3">
		<plugin
			type="org.mybatis.generator.plugins.EqualsHashCodePlugin"></plugin>
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin"></plugin>
		<plugin type="org.mybatis.generator.plugins.MapperConfigPlugin">
			<property name="targetProject" value="infrastructure\src\main\resources"></property>
			<property name="targetPackage" value="sqlmap"></property>
		</plugin>
		<plugin type="com.ymatou.demo1.mybatisgenerator.RenameModelPlugin">
			<property name="prefixes2Remove" value="tb, ymt, pp" />
			<!-- property name="suffix2Append" value="Po" /-->
		</plugin>
<!--		<plugin
			type="com.ymatou.coupon.mybatisgenerator.ForceCreateUpdateTimePlugin">
			<property name="insertTimeColumns" value="CreatedTime,dAddTime,CreateTime,AddTime" />
			<property name="lastUpdateTimeColumns" value="LastUpdatedTime,UpdatedTime,UpdateTime" />
			<property name="dbCurrentTimeExpr" value="getdate()" />
		</plugin>-->
		<commentGenerator
			type="com.ymatou.demo1.mybatisgenerator.DBCommentGenerator">
			<property name="suppressAllComments" value="false" />
		</commentGenerator>


		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://172.16.100.173:3306/test"
			userId="root" password="111111">
			<property name="remarksReporting" value="true" />
		</jdbcConnection>

		<javaModelGenerator targetPackage="com.ymatou.demo1.infrastructure.db.po"
			targetProject="infrastructure\src\main\java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="sqlmap.autogenerated"
			targetProject="infrastructure\src\main\resources">
		</sqlMapGenerator>
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.ymatou.demo1.infrastructure.db.mapper"
			targetProject="infrastructure\src\main\java">
		</javaClientGenerator>
		
		
		
		<table tableName="user_info" domainObjectName="UserInfoPo">
			<generatedKey column="id" sqlStatement="MySql"
				identity="true" />
		</table>
	</context>
</generatorConfiguration>